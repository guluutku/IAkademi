
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index5</title>

    @* Önce Css Sonra JavaScript olacak şekilde sıralanacak *@
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/css/style.css" rel="stylesheet" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
</head>
<body>

    <h3 class="xx pembe">3333333333333333</h3>
    <h3 class="xx pembe">33--------33</h3>
    <h4 class="xx pembe">4444444444444444</h4>
    <p class="xx pembe">p1111111111</p>
    <p class="xx pembe">p2222222222</p>
    <br />

    <button id="btn8">BİRDEN FAZLA REMOVECLASS</button>

</body>
</html>

<script>
    // Called every 1 second
    setInterval(function () {
        $("h3, h4 ,p").toggleClass("xx pembe")
    }, 1000)

